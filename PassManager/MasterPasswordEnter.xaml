<Window x:Class="PassManager.MasterPasswordEnter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PassManager"
        mc:Ignorable="d"
        Title="Ввод Мастер пароля" Height="233.953" Width="600.866">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="103"/>
            <ColumnDefinition Width="92"/>
        </Grid.ColumnDefinitions>
        <Label Content="Введите Мастер пароль:" Margin="10,23,10,0" VerticalAlignment="Top" Height="26" Grid.ColumnSpan="3"/>
        <TextBox Name="PassBox" Height="23" Margin="35,49,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Grid.ColumnSpan="3"/>
        <Button Content="Войти" Margin="7,0,10,10" Height="20" VerticalAlignment="Bottom" Grid.Column="2" Click="Ok_Click"/>
        <Button Content="Отмена" Margin="17,0,11,10" Height="20" VerticalAlignment="Bottom" Grid.Column="1" Click="Cancel_Click"/>

    </Grid>
</Window>
